
alias conf="sudo nano /etc/nixos/configuration.nix"
alias nxrs="sudo nixos-rebuild switch --flake /etc/nixos#default"
alias nxrt="sudo nixos-rebuild test --flake /etc/nixos#default"

alias vpntvup="sudo wg-quick up ~/TVWireguard.conf"
alias vpntvdown="sudo wg-quick down ~/TVWireguard.conf"

alias editrc="nano ~/.bashrc"
